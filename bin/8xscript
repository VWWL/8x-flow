#!/usr/bin/env node

const script = require('../lib/main.js');
const fs = require('fs');

const argv = require('yargs').argv;

if (argv._.length === 1) {
    let file = argv._[0];
    let yml = fs.readFileSync(`${process.cwd()}/${file}`).toString();
    let result = script.parse(yml);
    console.log(JSON.stringify(result, null, 2));
};
